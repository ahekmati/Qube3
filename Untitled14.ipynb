   "source": [
    "import numpy as np\n",
    "\n",
    "A = np.array([\n",
    "    [4, -5, 6],\n",
    "    [7, -8, 6],\n",
    "    [1.5, -0.5, -2]\n",
    "])\n",
    "\n",
    "# Compute eigenvalues and eigenvectors\n",
    "vals, vecs = np.linalg.eig(A)\n",
    "\n",
    "print(\"Eigenvalues:\")\n",
    "print(vals)\n",
    "print(\"\\nEigenvectors (each column is an eigenvector):\")\n",
    "print(vecs)"
   ]
